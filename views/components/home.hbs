<form action="/" method="post" enctype="multipart/form-data" class="mb-4" autocomplete="off">
    <div class="row">
        <div class="col-12 col-sm-9 col-md-9 col-lg-10 col-xl-10">
            <div class="form-group">
                <input type="text" id="searchThrough" placeholder="Search for item" name="searchThrough" class="form-control mb-4"/>
            </div>
        </div>
        <div class="col-12 col-sm-3 col-md-3 col-lg-2 col-xl-2">
            <button type="submit" class="btn btn-primary btn-block w-100">Search</button>
        </div>
    </div>
</form>
<h1 class="display-4">Misplaced</h1>
<div class="row mt-4">
    {{#each data}}
    <div class="col-12 col-sm-12 col-md-6 col-lg-3 col-xl-3">
        <div class="card w-100">
            <img src="{{imgUrl}}" alt="" height="185rem" class="card-img-top">
            <div class="card-body">
                <h5 class="card-title">{{productName}}</h5>
                <h6 class="card-subtitle mb-2 text-muted">Found at {{lostLocation}} [{{lostFloor}}]</h6>
                <p class="card-text">{{itemDescription}}</p>
            </div>
        </div>
    </div>
    {{/each}}
</div>